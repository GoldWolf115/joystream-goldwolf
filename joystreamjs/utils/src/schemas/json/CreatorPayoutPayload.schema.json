{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreatorPayoutPayload",
  "items": {
    "$ref": "#/definitions/CreatorPayout"
  },
  "type": "array",
  "definitions": {
    "CreatorPayout": {
      "additionalProperties": false,
      "properties": {
        "channelId": {
          "type": "number"
        },
        "cumulativePayoutOwed": {
          "type": "number"
        },
        "merkleBranch": {
          "items": {
            "$ref": "#/definitions/ProofElement"
          },
          "type": "array"
        },
        "payoutRationale": {
          "type": "string"
        }
      },
      "required": ["channelId", "cumulativePayoutOwed", "merkleBranch", "payoutRationale"],
      "type": "object"
    },
    "ProofElement": {
      "additionalProperties": false,
      "properties": {
        "hash": {
          "type": "string"
        },
        "side": {
          "$ref": "#/definitions/Side"
        }
      },
      "required": ["hash", "side"],
      "type": "object"
    },
    "Side": {
      "enum": [0, 1],
      "type": "number"
    }
  }
}
